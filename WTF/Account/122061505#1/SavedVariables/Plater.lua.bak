						["Initialization"] = "function (modTable)\n    \n    \n    --Settings:\n    local threshold = modTable.config.threshold or 0.2\n    local iconSize = abs(modTable.config.size) or 15\n    \n    local icon = tonumber(modTable.config.icon) or 613533\n    local anchor = {\n        side = abs(modTable.config.anchor) or 6,\n        --1 = topleft 2 = left 3 = bottomleft 4 = bottom 5 = bottom right 6 = right 7 = topright 8 = top\n        x = abs(modTable.config.xoffset) or 0, --x offset\n        y = abs(modTable.config.yoffset) or 0, --y offset\n    };\n    \n    function modTable.updateIcon(unitFrame)\n        if not unitFrame or unitFrame.ActorType == \"friendlyplayer\" or unitFrame.ActorType == \"friendlynpc\"  then\n            if unitFrame.executeIconIndicator then\n                unitFrame.executeIconIndicator:Hide();\n            end\n            return \n        end\n        \n        if not unitFrame.executeIconIndicator then\n            unitFrame.executeIconIndicatorFrame = CreateFrame (\"frame\",unitFrame:GetName() .. \"executeIconIndicatorFrame\", unitFrame, BackdropTemplateMixin and \"BackdropTemplate\")\n            unitFrame.executeIconIndicator = unitFrame.executeIconIndicatorFrame:CreateTexture(nil, \"OVERLAY\");\n            unitFrame.executeIconIndicator:SetTexture(icon);\n            unitFrame.executeIconIndicatorFrame:SetFrameLevel(unitFrame.healthBar:GetFrameLevel() + 25)\n            unitFrame.executeIconIndicator:Hide();\n        end\n        \n        Plater.SetAnchor(unitFrame.executeIconIndicator, anchor, unitFrame.healthBar)\n        unitFrame.executeIconIndicator:SetSize(iconSize, iconSize);\n        \n        if unitFrame.healthBar.currentHealth and unitFrame.healthBar.currentHealthMax then\n            local healthPercent = unitFrame.healthBar.currentHealth / unitFrame.healthBar.currentHealthMax\n            if healthPercent <= threshold then\n                unitFrame.executeIconIndicator:SetTexture(icon);\n                if iconSize < 1 then\n                    iconSize = unitFrame.healthBar:GetHeight() \n                end\n                \n                unitFrame.executeIconIndicator:SetSize(iconSize, iconSize);\n                unitFrame.executeIconIndicator:Show();\n            else\n                unitFrame.executeIconIndicator:Hide();\n            end\n        else\n            unitFrame.executeIconIndicator:Hide();\n        end\n    end\nend",
