						["custom"] = "function(allstates, event, ...)\n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        local timestamp, subEvent, hideCaster, sourceGUID, sourceName, sourceFlags, sourceRaidFlags, destGUID, destName, destFlags, destRaidFlags, spellID, _,_, auraType = ...\n        if subEvent == \"SPELL_AURA_APPLIED\"\n        and spellID == 350469 then\n            aura_env.counter = aura_env.counter + 1\n            local class = select(3, UnitClass(destName))\n            local priority = class and aura_env.priority[class] or 10\n            local duration = select(5, WA_GetUnitDebuff(destName, spellID))\n            if priority and duration then\n                aura_env.data[destName] = {priority, duration, aura_env.counter}\n                if destName == WeakAuras.me then\n                    C_Timer.After(0.1, function() WeakAuras.ScanEvents(\"CAUSESE_EVALUATE\", \"nilcheck\") end)\n                end\n            end\n        elseif subEvent == \"SPELL_AURA_REMOVED\"\n        and spellID == 350469\n        and destName == WeakAuras.me then\n            for k,v in pairs(allstates) do\n                v.show = false\n                v.changed = true\n            end\n            return true\n        elseif subEvent == \"SPELL_CAST_START\"\n        and spellID == 350469 then\n            aura_env.counter = 0\n            aura_env.data = {}\n        end\n    elseif event == \"ENCOUNTER_START\" then\n        aura_env.counter = 0\n        aura_env.data = {}\n    elseif event == \"CAUSESE_EVALUATE\" and ... then\n        local myPriority\n        local othersPriority\n        local myDuration\n        local othersDuration\n        local myCounter\n        for k,v in pairs(aura_env.data) do\n            if k == WeakAuras.me then\n                myPriority = v[1]\n                myDuration = v[2]\n                myCounter = v[3]\n            else\n                othersPriority = v[1]\n                othersDuration = v[2]\n            end\n        end\n        if myPriority == 10 and myPriority == othersPriority then\n            --pingpong strat\n            allstates[\"PP\"] = {\n                show = true,\n                changed = true,\n                name = myDuration == othersDuration and aura_env.assign[myCounter] or myDuration < othersDuration and \"GO FAR\" or \"GO CLOSE\"\n            }\n            return true\n        else\n            --massDispel strat\n            allstates[\"MD\"] = {\n                show = true,\n                changed = true,\n                name = myPriority == othersPriority and aura_env.assign[myCounter] or myPriority < othersPriority and \"GO FAR\" or \"GO CLOSE\"\n            }\n            return true\n        end\n    end\nend",
