						["custom"] = "function(allstates, event, ...)\n    if event == \"COMBAT_LOG_EVENT_UNFILTERED\" then\n        local timestamp, subEvent, hideCaster, sourceGUID, sourceName, sourceFlags, sourceRaidFlags, destGUID, destName, destFlags, destRaidFlags, spellID, _,_, auraType = ...\n        if subEvent == \"SPELL_AURA_APPLIED\"\n        and spellID == 350469 then\n            local class = select(3, UnitClass(destName))\n            local priority = class and aura_env.priority[class] or 10\n            if class and priority then\n                aura_env.mdStrat = priority < 10 and true\n                if priority == 10 then\n                    aura_env.counter = aura_env.counter + 1\n                end\n                if not aura_env.mdStrat and aura_env.counter == 2 then\n                    WeakAuras.ScanEvents(\"CAUSESE_SHOWSTRAT\", \"Pingpong\")\n                elseif aura_env.mdStrat then\n                    WeakAuras.ScanEvents(\"CAUSESE_SHOWSTRAT\", \"Massdispel\")\n                    aura_env.mdStrat = true\n                end\n            end\n        elseif subEvent == \"SPELL_AURA_REMOVED\"\n        and spellID == 350469 then\n            local state = allstates[\"Massdispel\"] or allstates[\"Pingpong\"]\n            if state then\n                state.count = state.count - 1\n                state.changed = true\n                if state.count <= 0 then\n                    state.show = false\n                    return true\n                end\n            end\n        elseif subEvent == \"SPELL_CAST_START\"\n        and spellID == 350469 then\n            aura_env.mdStrat = false\n            aura_env.counter = 0\n        end\n    elseif event == \"ENCOUNTER_START\" then\n        aura_env.counter = 0\n        aura_env.mdStrat = false\n    elseif event == \"CAUSESE_SHOWSTRAT\" then\n        local strat = ...\n        if strat then\n            allstates[strat] = {\n                show = true,\n                changed = true,\n                count = 2,\n                name = strat,\n            }\n            return true\n        end\n    end\nend",
