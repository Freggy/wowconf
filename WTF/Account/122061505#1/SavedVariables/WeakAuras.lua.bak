			["preferToUpdate"] = true,
			["customText"] = "function()\n    local options = WeakAuras.IsOptionsOpen()\n    if options then\n        aura_env.sortAndOffset(true)\n    end\n    \n    return aura_env.state.name1, aura_env.state.name2\nend\n\n\n\n",
			["shadowYOffset"] = -1,
			["anchorPoint"] = "CENTER",
			["zoom"] = 0,
			["cooldownSwipe"] = true,
			["displayText_format_p_time_format"] = 0,
