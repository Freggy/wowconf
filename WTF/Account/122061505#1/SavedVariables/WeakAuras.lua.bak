			["xOffset"] = 0,
			["preferToUpdate"] = true,
			["customText"] = "function()\n    local result = \"\"\n    if WeakAuras.IsOptionsOpen() then\n        result = 1\n    else\n        local state = aura_env.state\n        if state and state.name and state.unit then\n            local region = aura_env.region\n            local plate = C_NamePlate.GetNamePlateForUnit(state.unit)\n            if plate then\n                region:ClearAllPoints()\n                region:SetAnchor(\"BOTTOM\", plate, \"TOP\")\n                if aura_env.config.hidepriority then\n                    region:SetAlpha(0)\n                else\n                    region:SetAlpha(100)\n                end\n                \n                if state.name == aura_env.priority then\n                    \n                    region:SetGlow(true)\n                    if aura_env.config.automark and not GetRaidTargetIndex(state.unit) then\n                        SetRaidTarget(state.unit, 8)\n                    end\n                else\n                    region:SetGlow(false)\n                end\n                region:Show()\n            else\n                region:Hide()\n            end\n            result = state.name\n        end\n    end\n    return result\nend",
