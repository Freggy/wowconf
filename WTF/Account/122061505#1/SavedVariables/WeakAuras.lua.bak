					["custom"] = "aura_env.replacements = {\n  \"FRONT\",\n  \"LEFT\",\n  \"BACK LEFT\",\n  \"RIGHT\",\n  \"BACK MIDDLE\",\n  \"BACK RIGHT\",\n};\n\nfunction aura_env.print(...)\n  print(\"WA \"..aura_env.id..\" :\",...);\nend\n\nfunction aura_env.printError(...)\n  print(\"WA \"..aura_env.id..\" :\",...);\nend",
