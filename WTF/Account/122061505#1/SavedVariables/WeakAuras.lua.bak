						["custom"] = "function(allstates, event, ...)\n  if(event == \"FRAME_UPDATE\")then\n    if not aura_env.last or aura_env.last < GetTime() - 0.2 then\n      aura_env.last = GetTime();\n      if(#aura_env.debuffedPeople > 0)then\n        local anyInRange = false;\n        for i,dude in ipairs(aura_env.debuffedPeople)do\n          if(not UnitIsUnit(\"player\",dude))then\n            local range = WeakAuras.GetRange(dude);\n            if(range <= 8)then\n              anyInRange = true;\n              break;\n            end\n          end\n        end\n        local state = allstates[\"\"];\n        if(state and state.inRange ~= anyInRange)then\n          state.changed = true;\n          state.inRange = anyInRange;\n          \n          return true;\n        end\n      end\n    end\n  elseif(event == \"COMBAT_LOG_EVENT_UNFILTERED\")then\n    local _, subEvent, _, _, _, _, _ ,_, destName, _, _, spellID = ...;\n    \n    if(spellID == 361745)then\n      if(subEvent == \"SPELL_AURA_APPLIED\")then\n        table.insert(aura_env.debuffedPeople,destName);\n        if(#aura_env.debuffedPeople == 1)then\n          allstates[\"\"] = {\n            changed = true,\n            show = true,\n            \n            progressType = \"timed\",\n            expirationTime = GetTime() + 8,\n            duration = 8,\n            inRange = false,\n          };\n          \n          return true;\n        end\n      else\n        for i,dude in ipairs(aura_env.debuffedPeople)do\n          if(dude == destName)then\n            table.remove(aura_env.debuffedPeople,i);\n            break;\n          end\n        end\n        if(#aura_env.debuffedPeople == 0)then\n          local state = allstates[\"\"];\n          if(state)then\n            state.changed = true;\n            state.show = false;\n            \n            return true; \n          end\n        end\n      end\n    end\n  end\nend",
